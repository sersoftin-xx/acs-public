{
  "name": "sersoftin/acs",
  "description": "ACS - web-app, that can control access to third-party developers software.",
  "homepage": "http://sersoftin.xyz",
  "type": "project",
  "license": "MIT",
  "require": {
    "php": ">=5.4.16",
    "cakephp/cakephp": "~3.2",
    "mobiledetect/mobiledetectlib": "2.*",
    "cakephp/migrations": "~1.0",
    "cakephp/plugin-installer": "*"
  },
  "require-dev": {
    "psy/psysh": "@stable",
    "cakephp/debug_kit": "~3.2",
    "cakephp/bake": "~1.1"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src"
    }
  },
  "scripts": {
    "post-install-cmd": "App\\Console\\Installer::postInstall",
    "post-autoload-dump": "Cake\\Composer\\Installer\\PluginInstaller::postAutoloadDump"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
